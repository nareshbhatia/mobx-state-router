{
  "name": "relative-paths",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://archfirst.org/myapp",
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rimraf deploy",
    "build": "react-scripts build",
    "postbuild": "yarn copy-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "copy-build": "run-script-os",
    "copy-build:win32": "echo '***** Copy build/* to deploy/myapp *****'",
    "copy-build:darwin:linux": "mkdir deploy && cp -R ./build ./deploy && mv ./deploy/build ./deploy/myapp",
    "format": "prettier --write README.md '{src,server}/**/{*.md,*.json,*.js,*.ts*,*.css}'",
    "serve": "node server"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mobx": "^6.0.0",
    "mobx-react-lite": "^3.0.0",
    "mobx-state-router": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.15",
    "@types/jest": "26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.1",
    "typescript": "^3.9.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
